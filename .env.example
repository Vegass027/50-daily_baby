# Solana RPC Configuration
SOLANA_RPC_URL=https://api.mainnet-beta.solana.com
SOLANA_RPC_URL_BACKUP=https://solana-api.projectserum.com
ALCHEMY_SOLANA_RPC=https://solana-mainnet.g.alchemy.com/v2/YOUR_ALCHEMY_API_KEY
QUICKNODE_RPC_URL=https://YOUR_QUICKNODE_ENDPOINT.solana-mainnet.quiknode.pro/YOUR_API_KEY
HELIUS_RPC_URL=https://YOUR_HELIUS_ENDPOINT.helius-rpc.com/?api-key=YOUR_API_KEY

# Jito Configuration
JITO_AUTH_KEYPAIR_PRIVATE_KEY=YOUR_JITO_AUTH_KEYPAIR_BASE64
JITO_TIP_MULTIPLIER=1.15

# Supabase Configuration
DATABASE_URL=postgresql://postgres:[YOUR-PASSWORD]@db.[PROJECT-REF].supabase.co:5432/postgres
SUPABASE_URL=https://[PROJECT-REF].supabase.co
SUPABASE_ANON_KEY=YOUR_SUPABASE_ANON_KEY
SUPABASE_SERVICE_ROLE_KEY=YOUR_SUPABASE_SERVICE_ROLE_KEY

# Supabase MCP Configuration
SUPABASE_PROJECT_ID=ocgnklghukdpefnekzhy
USE_DIRECT_DB_CLIENT=true

# Telegram Bot Configuration
TELEGRAM_BOT_TOKEN=YOUR_TELEGRAM_BOT_TOKEN
TELEGRAM_ADMIN_USER_IDS=123456789,987654321
ALLOWED_TELEGRAM_USERS=123456789,987654321

# Security Configuration
ENCRYPTION_KEY=YOUR_ENCRYPTION_KEY_MIN_32_CHARS
MASTER_PASSWORD=YOUR_MASTER_PASSWORD_FOR_WALLET_ENCRYPTION
BACKUP_ENCRYPTION_KEY=YOUR_64_CHAR_HEX_KEY_HERE  # Generate: openssl rand -hex 32
WALLET_BACKUP_DIR=./backups/wallets

# Trading Configuration
DEFAULT_SLIPPAGE=0.01
DEFAULT_PRIORITY_FEE_MODE=normal
MAX_RETRIES=3
MEV_PROTECTION=true

# Monitoring Configuration
LOG_LEVEL=info
LOG_TO_FILE=true
LOG_FILE_PATH=./logs
ENABLE_METRICS=true

# Webhook Configuration (optional)
WEBHOOK_URL=https://your-webhook-url.com/webhook
WEBHOOK_SECRET=YOUR_WEBHOOK_SECRET

# Development Configuration
NODE_ENV=production
ENABLE_DEVNET=false
ENABLE_DRY_RUN=false
