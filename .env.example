# Telegram
TELEGRAM_BOT_TOKEN=your_telegram_bot_token_here
ALLOWED_TELEGRAM_USERS=7295309649

# Master password для шифрования кошельков
MASTER_PASSWORD=your_secure_password_here

# Solana RPC (Mainnet)
# Основной RPC для Alchemy. bot.ts также может использовать QUICKNODE_RPC_URL как запасной.
ALCHEMY_SOLANA_RPC="https://solana-mainnet.g.alchemy.com/v2/YOUR_API_KEY"
QUICKNODE_RPC_URL=""

# Приватный ключ кошелька (НЕ РЕКОМЕНДУЕТСЯ)
# Бот использует WalletManager для безопасного хранения.
# Эта переменная здесь для соответствия плану, но не используется в текущей реализации.
WALLET_PRIVATE_KEY=

# Jupiter API (пока не используется)
JUPITER_API_KEY=your_jupiter_api_key_here
JUPITER_API_URL=https://api.jup.ag

# Jito (в Devnet не используется)
# JITO_RPC_URL=

# Сеть
SOLANA_NETWORK=mainnet-beta

# Настройки по умолчанию
DEFAULT_SPEED_STRATEGY=normal
DEFAULT_MEV_PROTECTION=true
DEFAULT_SLIPPAGE=1.0

# Webhook для алертов (Discord/Slack)
WEBHOOK_URL=https://discord.com/api/webhooks/xxx

# Dry-run mode (true для тестирования)
DRY_RUN_MODE=true

# Database (Supabase PostgreSQL)
# Получить в Supabase Dashboard: Settings → Database → Connection String → URI (Transaction mode)
DATABASE_URL="postgresql://postgres.[ref]:[password]@aws-0-eu-west-1.pooler.supabase.com:6543/postgres"

# Supabase
# Получить в Supabase Dashboard: Settings → API
SUPABASE_URL="https://[project-ref].supabase.co"
SUPABASE_ANON_KEY="your_anon_key_here"
SUPABASE_SERVICE_ROLE_KEY="your_service_role_key_here"
