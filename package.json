{
  "name": "solana-dex-bot",
  "version": "1.0.0",
  "description": "A production-ready Solana DEX trading bot with a Telegram interface.",
  "main": "dist/bot.js",
  "scripts": {
    "start": "node dist/bot.js",
    "build": "tsc",
    "dev": "ts-node src/bot.ts",
    "render-build": "npm install && npm run migrate:deploy && npm run build",
    "migrate:deploy": "prisma migrate deploy",
    "migrate:dev": "prisma migrate dev",
    "migrate-to-db": "ts-node scripts/migrate-to-database.ts",
    "test": "jest --forceExit",
    "test:watch": "jest --watch --forceExit",
    "test:coverage": "jest --coverage --forceExit",
    "test:unit": "jest tests/unit --forceExit",
    "test:integration": "jest tests/integration --forceExit",
    "test:devnet": "jest tests/integration/devnet.test.ts --forceExit",
    "test:mainnet": "jest tests/integration/mainnet.test.ts --forceExit",
    "test:realtime": "npx tsx src/tests/realtimeTest.ts",
    "test:vitest": "vitest run"
  },
  "keywords": [
    "solana",
    "dex",
    "trading-bot",
    "telegram",
    "typescript"
  ],
  "author": "Kilo Code",
  "license": "MIT",
  "dependencies": {
    "@coral-xyz/anchor": "^0.29.0",
    "@jup-ag/api": "^6.0.48",
    "@prisma/adapter-better-sqlite3": "^7.2.0",
    "@prisma/adapter-pg": "^7.2.0",
    "@prisma/client": "^7.2.0",
    "@solana/spl-token": "^0.4.8",
    "@solana/web3.js": "^1.95.0",
    "@supabase/supabase-js": "^2.89.0",
    "axios": "^1.7.2",
    "better-sqlite3": "^12.5.0",
    "bs58": "^5.0.0",
    "dotenv": "^16.4.5",
    "jito-ts": "^4.1.0",
    "pg": "^8.11.3",
    "pumpdotfun-repumped-sdk": "^1.0.0",
    "pumpdotfun-sdk": "^1.4.2",
    "telegraf": "^4.16.3",
    "telegraf-ratelimit": "^2.0.0",
    "tweetnacl": "^1.0.3",
    "winston": "^3.11.0",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/better-sqlite3": "^7.6.13",
    "@types/jest": "^30.0.0",
    "@types/node": "^20.0.0",
    "@types/pg": "^8.11.0",
    "@vitest/ui": "^4.0.16",
    "jest": "^30.2.0",
    "prisma": "^7.2.0",
    "ts-jest": "^29.4.6",
    "ts-node": "^10.9.2",
    "typescript": "^5.4.5",
    "vitest": "^4.0.16"
  }
}
